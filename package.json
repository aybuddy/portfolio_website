{
	"name": "creative_Portfolio",
	"version": "1.0.0",
	"description": "Will use this to create my portfolio using HTML, CSS, and JS. \r For CSS, I will use SASS and compile it to CSS.",
	"main": "index.js",
	"scripts": {
		"watch:sass": "node-sass sass/main.scss css/style.css -w",
		"start": "npm-run-all --parallel watch:sass",
		"compile:sass": "node-sass sass/main.scss css/style.comp.css",
		"concat:css": "concat -o css/style.concat.css css/style.comp.css",
		"prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.concat.css -o css/style.prefix.css",
		"compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
		"build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/aybuddy/creative_Portfolio.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/aybuddy/creative_Portfolio/issues"
	},
	"homepage": "https://github.com/aybuddy/creative_Portfolio#readme",
	"devDependencies": {
		"autoprefixer": "^9.6.0",
		"concat": "^1.0.3",
		"node-sass": "^4.12.0",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^6.1.2"
	},
	"dependencies": {
		"animejs": "^3.0.1"
	}
}
